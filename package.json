{
    "name": "react-redux-simple-generator",
    "version": "1.0.0",
    "description": "react redux starter",
    "scripts": {
        "build": "./node_modules/.bin/rollup -c rollup.config.js",
        "build:dev": "./node_modules/.bin/rollup -c rollup.config.js -w",
        "browse": "browser-sync start --s --ss build --index html/index-dev.html --files html/**/*.html,build/**/*.js --no-notify",
        "start": "npm-run-all --parallel build:dev browse",
        "test": "mocha --compilers js:babel-core/register 'src/**/*.test.js' --recursive",
        "test:watch": "npm run test -- --watch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/duncan60/react-redux-simple-generator"
    },
    "keywords": ["react", "reactjs", "redux", "webpack", "rollup"],
    "author": "Duncan Du",
    "license": "MIT",
    "devDependencies": {
        "app-module-path": "^2.1.0",
        "autoprefixer": "^6.6.1",
        "babel": "^6.5.2",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.14.0",
        "babel-eslint": "^6.1.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-plugin-import": "^1.0.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2015-rollup": "^3.0.0",
        "babel-preset-react": "^6.11.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.24.1",
        "browser-sync": "^2.18.13",
        "chai": "^3.5.0",
        "eslint": "^2.8.0",
        "eslint-plugin-react": "^5.0.1",
        "less": "^2.7.1",
        "mocha": "^2.4.5",
        "node-sass": "^3.10.1",
        "rollup": "0.52.1",
        "rollup-plugin-babel": "3.0.2",
        "rollup-plugin-commonjs": "8.2.6",
        "rollup-plugin-node-globals": "1.1.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-replace": "2.0.0",
        "rollup-plugin-uglify": "2.0.1",
        "rollup-watch": "4.3.1"
    },
    "dependencies": {
        "es6-promise": "^4.1.1",
        "isomorphic-fetch": "^2.2.1",
        "npm-run-all": "^4.1.2",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-addons-shallow-compare": "^15.6.2",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "redux": "^3.7.2"
    }
}